    <div class="cheetos-featured-product--container-bg-desktop">
    <div class="carousel_fullwidth carousel_desktop js-main-carousel-cheetos">
        {% for product in featured_products %}
        <div class="banner_slide home_slide" data-nid="{{nid}}">
            <div class="banner-desktop">
                    {% if featured_products|length and loop.first  %}
                        <h2>{{'Featured product'|t}}</h2>
                    {% endif %}

                        <div class="cheetos-featured--container">
                            <div class="cheetos-feature--display">
                                <div class="cheetos-feature--leftcol">
                                    <img loading="lazy" class="product-image" src="{{product.extra_image_url}}" alt="{{ product.title | raw}}">
                                </div>
                            </div>
                            <div class="cheetos-coupon--text">

                                <div class="cheetos-banner_slide--text">

                                    <span class="product-title"> {{product.title|raw}}</span>
                                    {% set trimmed_body = truncateText(product.body|raw, 20) %}

                                    {# <span class="cheetos-product-description"> {{product.body|raw}}</span> #}

                                    <br>
                                    <div class="cheetos-btn">
                                        <a href="{{product.link}}" title ="{{ product.title | raw}}">
                                            {{'View Product'|t}}
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                </div>

            </div>
        </div>
    {% endfor %}
    </div>
</div>










