    <section class="crispyminis-product-banner-block">
        <h1>{{'Products'|t}}</h1>
        <p class="crispyminis-product-banner-suggest-serving">{{'Suggested serving'|t}}</p>
    </section>


    <section id="product-categories" class="crispyminis-products">
         <div class="js-product-carousel">

        <div class="container">
            <div class="crispyminis-product-category">
                {% for category in product_categories %}
                    <div class="crispyminis-product-box {{category.background_color}}">
                        <div class="crispyminis-product-img">
                            <a href="{{category.link}}" hreflang="en">
{#                                <img src="{{category.image_url}}" width="369" height="514" alt="Crispyminis Chips" typeof="foaf:Image" class="crispyminis-products-image-hover">#}
