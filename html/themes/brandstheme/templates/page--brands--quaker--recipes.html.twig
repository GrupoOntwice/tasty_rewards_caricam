<section id="recipes" class="quaker-recipes-main-block">

  <div class="container">
    <div class="row">
        <div class="row js-recipe-container-backup hidden">
        </div>

      <div class="col-md-12">
        <div class="row js-recipe-container" data-count="{{ recipe_count }}">

          {% for recipe in recipes %}
            <div class="col-md-4 quaker-recipes-col text-center">

              <div class="row">
                <div class="col-md-12 quaker-recipes-img-block">
                  <a href="{{ recipe.link }}" aria-label="{{ recipe.title|raw }}">
                    <div class="quaker-recipes-img {% if recipe.img_position is empty %} img-align-center {% else %} img-align-{{recipe.img_position}} {% endif %}" style="background-image: url('{{ recipe.image_url }}')">
                    </div>
                  </a>
                </div>
              </div>

              <div class="quaker-stars quaker-stars-corner">
                {% for i in 1..5 %}
                  <span {% if i <= recipe.rating %}class="checked"{% endif %}></span>
                {% endfor %}
              </div>

              <div class="quakers-recipes-description text-center">

                <a href="{{ recipe.link }}" aria-label="{{ recipe.title|raw }}">
                  <h4>
                    {{ recipe.title|raw }}
                  </h4>
                </a>

                {# <p>{{ recipe.body|raw }}</p> #}

              </div>

              <div class="quaker-recipes-btn">
                <a href="{{ recipe.link }}" class="btn_quaker">
                  {{ 'View Recipe'|t }}
                </a>
              </div>

            </div>
          {% endfor %}

        </div>
      </div>

      <div class="col-md-12 padding-top-medium text-center">
        <a href="javascript:void(0);" class="more-recipes btn_quaker-white">{{ 'View More Recipes'|t }}</a>
      </div>

    </div>

  </div>


</section>


<section id="our-recipes" class="quaker-recipes-block">

  <div class="quaker-featured-recipes-bg-left">
    <img class="" src="/themes/brandstheme/src/images/quaker/quaker-featured-recipes-bg-left.png" alt="Quaker Recipes block background">
  </div>

  {% include '@brandstheme/includes/featured/featured-recipes-quaker.html.twig' %}

  <div class="quaker-featured-recipes-bg-right">
    <img class="" src="/themes/brandstheme/src/images/quaker/quaker-featured-recipes-bg-right.png" alt="Quaker Recipes block background">
  </div>

</section>

<section id="coupon" class="quaker-coupon-block">
  {{ coupon_block|t|raw }}
</section>

