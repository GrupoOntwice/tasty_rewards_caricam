<script type="text/javascript">
</script>

{{ attach_library( 'pepsibrands/realjoy_export' ) }}

<h2 class=""> {{data.title}} </h2>

<div class="view-filters">
   <form class="views-exposed-form" data-drupal-selector="views-exposed-form-content-page-1" method="POST" id="views-exposed-form-content-page-1" accept-charset="UTF-8">
      <div class="form--inline clearfix">
         <div class="js-form-item form-item js-form-type-select form-type-select js-form-item-type form-item-type">
            <label for="edit-contestid">{{'Country'|t}}</label>
            <select data-drupal-selector="edit-country" id="edit-country" name="country" class="form-select" required>
                  <option value="" {% if data.nofilter == 1 %} selected="selected" {% endif %} >- Any -</option>
               {% for country in data.countries %}
                  <option value="{{country}}" {% if country == data.selected_country %} selected="selected" {% endif %} >{{country}}</option>
               {% endfor %}
            </select>
         </div>

         <div class="js-form-item form-item js-form-type-date form-type-date js-form-item-field-opening-date-0-value-date form-item-field-opening-date-0-value-date form-no-label">
            <label for="start_date" class="js-form-required">{{'Start Date'|t}}</label>

            <input data-drupal-selector="edit-field-opening-date-0-value-date" title="Date (par ex. 2021-09-27)" type="date" min="1900-01-01" max="2050-12-31" data-drupal-date-format="Y-m-d" id="start_date" name="start_date" value="{{data.start_date}}" size="12" class="form-date"  aria-required="true" required>
         </div>

         <div class="js-form-item form-item js-form-type-date form-type-date js-form-item-field-closing-date-0-value-date form-item-field-closing-date-0-value-date form-no-label">
            <label for="end_date" class="js-form-required">{{'End date'}}</label>
            <input data-drupal-selector="edit-field-closing-date-0-value-date" title="Date (par ex. 2021-09-27)" type="date" min="1900-01-01" max="2050-12-31" data-drupal-date-format="Y-m-d" id="end_date" name="end_date" value="{{data.end_date}}" size="12" class="form-date" required>
         </div>

         <div data-drupal-selector="edit-actions" class="form-actions js-form-wrapper form-wrapper" id="edit-actions"><input data-drupal-selector="edit-submit-content" type="submit" id="edit-submit-content" value="Show Stats" class="button js-form-submit form-submit"></div>
      </div>
   </form>
</div>

{# {% include '@pepsicontest/includes/contest_stats_filters.html.twig' %} #}

<div class="container">
	<table class="views-table views-view-table cols-9 responsive-enabled sticky-enabled">
   <thead>
      <tr>
         <th id="view-title-table-column" class="views-field views-field-title" scope="col">{{'Source ID'|t}}</th>
         <th id="view-title-table-column" class="views-field views-field-title" scope="col">{{'Total signups'|t}}</th>
         <th id="view-title-table-column" class="views-field views-field-title" scope="col">{{'Subscription PTR'|t}}</th>
         <th id="view-title-table-column" class="views-field views-field-title" scope="col">{{'Subscription Snacks'|t}}</th>
      </tr>
   </thead>
   <tbody>
	{% for entry in data.sourceids %}
      <tr>
         <td headers="view-title-table-column" class="views-field views-field-title">{{entry.source}} </td>
         <td headers="view-title-table-column" class="views-field views-field-title">{{entry.total}} </td>
         <td headers="view-title-table-column" class="views-field views-field-title">{{entry.sub_ptr}} </td>
         <td headers="view-title-table-column" class="views-field views-field-title">{{entry.sub_snacks}} </td>
      </tr>
	{% endfor %}
   </tbody>
</table>

{# <input data-drupal-selector="edit-clear" type="submit" id="edit-download" name="op" value="Export Csv" class="button js-export-csv  form-submit" data-link="{{data.csv_link}}"> #}


</div>
