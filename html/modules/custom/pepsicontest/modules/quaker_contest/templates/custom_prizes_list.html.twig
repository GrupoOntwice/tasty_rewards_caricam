{{ attach_library( 'quaker_contest/landing' ) }}
<script type="text/javascript">
  window.popup_contest = 1;
</script>
{% if data.contest_type == 'hockey' %}
  {{ attach_library( 'quaker_contest/hockey' ) }}
  {% include '@quaker_contest/include/contest-prizes-hockey.html.twig' %}
{% else %}
  {{ attach_library( 'quaker_contest/grabsnack' ) }}



<style>


  .prizes-banner {
    background-image: url('/themes/tastytheme/src/images/grab-snack/thankyou-background.jpg');
    background-size: cover;
    background-repeat: no-repeat;
    background-position: 50% 50%;
  }

  .button-style {
    background-color: #F40522;
    border: 1px solid #F40522;
    color: #fff;
    font-family: 'OpenSans-semibold';
    padding: 8px;
    padding-right: 25px;
    padding-left: 25px;
    margin-top: 20px;
  }

  .mw-100 {
    max-width: 100%;
  }

  .mw-50 {
    max-width:50%;
  }

  .color-black {
    color: #000;
  }

  .faq-btn {
    padding-top: 2rem;
  }

  .w-100 {
    width: 100%;
  }

  .prizes-red-text {
    width: 100%;
    text-align: center;
    padding: 2rem;
  }

.prizes-row {
  padding-bottom:30px;
}




  .p-3 {
    padding: 5rem;
  }

  .color-link {
    color:#F30522;
  }

  .grabsnack-prizes {
    margin: auto !important;
    padding-left: 2rem;
  }

  .all-prizes {
    padding-top: 2rem;
  }

  .decrease-maxwidth {
    max-width: 75%;
  }

  .prices-available {
    background-image: url("/themes/tastytheme/src/images/grab-snack/yellow-background.jpg");
  }

  .prizes-btn {
    width: 80%;
  }

  .color-lightblue {
    color:#17B3F1
  }

  .prize-towel {
    width: 82%;
  }

  .grabsnack-prizes {
    display: flex;
  }

  .montserrat-black {
    font-family: 'Montserrat-black';
  }

  .text-center {
    text-align: center;
  }

  .text-uppercase {
    text-transform: uppercase;
  }

  .price-img {
    max-width: 100% !important;
    margin: 20px 0;
  }

  .price-name {
    padding-top: 1rem;
    padding-bottom: 1rem;
  }

  .free-coupon img{
    width: 33%;
  }




  @media (max-width: 967px) {
    .prize-btn {
      width: 147px !important;
    }
    .grabsnack-prizes {
      padding-left: 0 !important;
    }

    .grabsnack-prizes p br {
      display: none;
    }

    .grabsnack-prizes p, .grabsnack-prizes h1{
      text-align: center;
    }

    .prizes-btn {
      text-align: center;
    }
    .free-coupon{
      padding: 0;
    }
    .free-coupon img {
      max-width: 75%;
      width: 100%;
    }

    .prizes-btn {
      width:100%;
    }

    .prizes-row {
      padding-top: 2rem;
    }
  }




</style>


<section class = "prizes-banner">
  <div class = "container-fluid">

    <div class = "row prizes-row">


      <div class = "col-lg-6 grabsnack-prizes m-auto pl-5 ">
