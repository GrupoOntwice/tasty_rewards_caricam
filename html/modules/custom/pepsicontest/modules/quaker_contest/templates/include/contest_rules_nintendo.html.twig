{% include '@quaker_contest/tracking/pixel-tracking-nintendo2.html.twig' %}
{{ attach_library( 'quaker_contest/landing' ) }}
{{ attach_library( 'quaker_contest/nintendo' ) }}
<script type="text/javascript">
window.popup_contest = 1;
</script>

<section class="nintendo-contest-rules">
    <div class="nintendo-contest-rules-text-banner">
        <h1>{{ "Official Rules" | t}}</h1>
    </div>
</section>

<section id="contentBlocks" class="container-fluid nintendo-rules">
    <div class="container">
         {{ data.body | raw}}
    </div>
</section>

<section class="nintendo-landing-contest-rules">

  <img class = "logo-nintendo two-logos" src="/themes/tastytheme/src/images/contest-nintendo/nintendo-microsite/logo.png" alt = "logo">

  <div class="container">
    <div class="margin-bottom margin-top">
      {{data.legal_footer|raw}}
    </div>
    {#		<span class="rules">#}
    {#			<a href="{{data.rules_link}}">{{'Read our complete rules'|t}}</a>#}
    {#		</span>#}
    <div class="nintendo-landing-contest-rules-text-wrap">
      <a class="btn_nintendo rules_btn" href="{{ data.rules_link }}">{{'Official Rules'|t}}</a><br>
      <a class="btn_nintendo" href="{{ data.faq_link }}">{{'FAQ'|t }}</a>
      <br>

    </div>
  </div>
</section>

